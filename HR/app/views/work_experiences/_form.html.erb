<%= form_for([@resume, @resume.work_experiences.build]) do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :education %><br>
    <%= f.text_field :education %>
  </p>
    <p>
    <%= f.label :year_start %><br>
    <%= f.text_field :year_start %>
  </p>
    <p>
    <%= f.label :year_end %><br>
    <%= f.text_field :year_end %>
  </p>
    <p>
    <%= f.label :about %><br>
    <%= f.text_area :about %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>